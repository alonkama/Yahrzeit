    <validationRules>
        <fullName>Either_Hebrew_or_Gregorian_not_both</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
AND(NOT(ISPICKVAL( Day_Gregorian__c, &quot;&quot;)) , NOT(ISPICKVAL( Day_Hebrew__c, &quot;&quot;))),
AND(NOT(ISBLANK(  Year_Gregorian__c )) , NOT(ISBLANK(  Year_Hebrew__c ))))</errorConditionFormula>
        <errorMessage>Fill in either Hebrew or Gregorian date, not both.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Gregorian_year_in_range</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK(  Year_Gregorian__c ) , FALSE, 

OR (VALUE(Year_Gregorian__c ) &lt; 1800, VALUE(Year_Gregorian__c ) &gt; 2100 ))</errorConditionFormula>
        <errorDisplayField>Year_Gregorian__c</errorDisplayField>
        <errorMessage>Gregorian year must be between 1800 and 2100.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Gregorian_year_is_number</fullName>
        <active>true</active>
        <errorConditionFormula>IF (ISBLANK(  Year_Gregorian__c ) , FALSE, NOT(ISNUMBER(Year_Gregorian__c  )))</errorConditionFormula>
        <errorDisplayField>Year_Gregorian__c</errorDisplayField>
        <errorMessage>You must enter a valid year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Hebrew_year_in_range</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK(  Year_Hebrew__c ) , FALSE, 

OR (VALUE(Year_Hebrew__c ) &lt; 5000, VALUE(Year_Hebrew__c ) &gt; 6000 ))</errorConditionFormula>
        <errorDisplayField>Year_Hebrew__c</errorDisplayField>
        <errorMessage>Hebrew year must be between 5000 and 6000</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Hebrew_year_is_number</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK(  Year_Hebrew__c ) , FALSE, NOT(ISNUMBER(Year_Hebrew__c  )))</errorConditionFormula>
        <errorDisplayField>Year_Hebrew__c</errorDisplayField>
        <errorMessage>You must enter a number for Hebrew year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Gregorian_then_day_and_month</fullName>
        <active>false</active>
        <errorConditionFormula>OR(  AND(NOT(ISPICKVAL( Day_Gregorian__c, &quot;&quot;)) , ISPICKVAL ( Month_Gregorian__c , &quot;&quot;)),
         AND(NOT(ISPICKVAL( Month_Gregorian__c, &quot;&quot;)) , ISPICKVAL ( Day_Gregorian__c , &quot;&quot;)))</errorConditionFormula>
        <errorMessage>If using Gregorian date, must fill both day and month.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Hebrew_then_day_and_month</fullName>
        <active>true</active>
        <errorConditionFormula>OR(  AND(NOT(ISPICKVAL( Day_Hebrew__c, &quot;&quot;)) , ISPICKVAL ( Month_Hebrew__c , &quot;&quot;)),
         AND(NOT(ISPICKVAL( Month_Hebrew__c, &quot;&quot;)) , ISPICKVAL ( Day_Hebrew__c , &quot;&quot;)))</errorConditionFormula>
        <errorMessage>If using Hebrew date, must fill both day and month.</errorMessage>
    </validationRules>
